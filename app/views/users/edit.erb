<h2>ユーザー編集</h2>


<%= form_with model: @user, url: users_path, local: true, method: :patch do |f| %>

	<%= f.label :name, "Username" %>
		<%= f.text_field :name, class: "form-box" %>

			<%= f.label :email, "Email" %>
				<%= f.email_field :email, class: "form-box" %>

							<%= f.submit "更新" , class: "btn" %>


								<% end %>